    <types:type name="companies_json" format="json">
      <types:shape format="weave" example="examples/companies.json"><![CDATA[%dw 2.0

type companies_json = Array<{|  id: Number {"typeId": "int"},   name: String|}> {"example": "[{\\\"id\\\":1,\\\"name\\\":\\\"First Company\\\"},{\\\"id\\\":2,\\\"name\\\":\\\"Second Company\\\"}]"}

]]></types:shape>
    </types:type>
    <types:type name="company_json" format="json">
      <types:shape format="weave" example="examples/company.json"><![CDATA[%dw 2.0

type company_json = {|
  id: Number {"typeId": "int"}, 
  name: String, 
  creationDate: String
|} {"example": "{\\\"id\\\":1,\\\"name\\\":\\\"First Company\\\",\\\"creationDate\\\":\\\"2019-11-06\\\"}"}

]]></types:shape>
    </types:type>
    <types:type name="department_json" format="json">
      <types:shape format="weave" example="examples/department.json"><![CDATA[%dw 2.0

type department_json = {|
  id: Number {"typeId": "int"}, 
  name: String, 
  companyId: Number {"typeId": "int"}, 
  creationDate: String
|} {"example": "{\\\"id\\\":1,\\\"name\\\":\\\"First Department\\\",\\\"companyId\\\":1,\\\"creationDate\\\":\\\"2019-11-06\\\"}"}

]]></types:shape>
    </types:type>
    <types:type name="employee_json" format="json">
      <types:shape format="weave" example="examples/employee.json"><![CDATA[%dw 2.0

type employee_json = {|
  id: Number {"typeId": "int"}, 
  firstName: String, 
  lastName: String, 
  age: Number {"typeId": "int"}, 
  passportNumber: String, 
  phoneNumber: String, 
  address: String, 
  companyId: Number {"typeId": "int"}, 
  departmentId: Number {"typeId": "int"}, 
  position: String, 
  login: String, 
  password: String, 
  roles: String, 
  creationDate: String
|} {"example": "{\\\"id\\\":9,\\\"firstName\\\":\\\"Alexander\\\",\\\"lastName\\\":\\\"Kolosov\\\",\\\"age\\\":41,\\\"passportNumber\\\":\\\"AA888888\\\",\\\"phoneNumber\\\":\\\"+37369888888\\\",\\\"address\\\":\\\"MD\\\",\\\"companyId\\\":2,\\\"departmentId\\\":3,\\\"position\\\":\\\"java developer\\\",\\\"login\\\":\\\"alexander\\\",\\\"password\\\":\\\"kolosov\\\",\\\"roles\\\":\\\"employee\\\",\\\"creationDate\\\":\\\"2019-11-06\\\"}"}

]]></types:shape>
    </types:type>
    <types:type name="departments_json" format="json">
      <types:shape format="weave" example="examples/departments.json"><![CDATA[%dw 2.0

type departments_json = Array<{|  id: Number {"typeId": "int"},   name: String,   companyId: Number {"typeId": "int"}|}> {"example": "[{\\\"id\\\":1,\\\"name\\\":\\\"First Department\\\",\\\"companyId\\\":1},{\\\"id\\\":2,\\\"name\\\":\\\"First Department\\\",\\\"companyId\\\":2}]"}

]]></types:shape>
    </types:type>
    <types:type name="employees_json" format="json">
      <types:shape format="weave" example="examples/employees.json"><![CDATA[%dw 2.0

type employees_json = Array<{|  id: Number {"typeId": "int"},   firstName: String,   lastName: String,   companyId: Number {"typeId": "int"},   departmentId: Number {"typeId": "int"},   position: String|}> {"example": "[{\\\"id\\\":9,\\\"firstName\\\":\\\"Alexander\\\",\\\"lastName\\\":\\\"Kolosov\\\",\\\"companyId\\\":2,\\\"departmentId\\\":1,\\\"position\\\":\\\"java developer\\\"},{\\\"id\\\":5,\\\"firstName\\\":\\\"Barack\\\",\\\"lastName\\\":\\\"Obama\\\",\\\"companyId\\\":2,\\\"departmentId\\\":3,\\\"position\\\":\\\"president\\\"}]"}

]]></types:shape>
    </types:type>
    <types:type name="vacation_json" format="json">
      <types:shape format="weave" example="examples/vacation.json"><![CDATA[%dw 2.0

type vacation_json = {|
  id: Number {"typeId": "int"}, 
  employeeId: Number {"typeId": "int"}, 
  companyId: Number {"typeId": "int"}, 
  departmentId: Number {"typeId": "int"}, 
  startDate: String, 
  periodOfDays: Number {"typeId": "int"}, 
  status: String, 
  creationDate: String
|} {"example": "{\\\"id\\\":1,\\\"employeeId\\\":9,\\\"companyId\\\":2,\\\"departmentId\\\":3,\\\"startDate\\\":\\\"2020-05-23\\\",\\\"periodOfDays\\\":25,\\\"status\\\":\\\"In process\\\",\\\"creationDate\\\":\\\"2019-11-06\\\"}"}

]]></types:shape>
    </types:type>
    <types:type name="vacations_json" format="json">
      <types:shape format="weave" example="examples/vacations.json"><![CDATA[%dw 2.0

type vacations_json = Array<{|  id: Number {"typeId": "int"},   employeeId: Number {"typeId": "int"},   companyId: Number {"typeId": "int"},   departmentId: Number {"typeId": "int"},   startDate: String,   periodOfDays: Number {"typeId": "int"},   status: String|}> {"example": "[{\\\"id\\\":1,\\\"employeeId\\\":9,\\\"companyId\\\":2,\\\"departmentId\\\":3,\\\"startDate\\\":\\\"2020-05-23\\\",\\\"periodOfDays\\\":25,\\\"status\\\":\\\"In process\\\"},{\\\"id\\\":1,\\\"employeeId\\\":59,\\\"companyId\\\":2,\\\"departmentId\\\":3,\\\"startDate\\\":\\\"2020-06-23\\\",\\\"periodOfDays\\\":15,\\\"status\\\":\\\"In process\\\"}]"}

]]></types:shape>
